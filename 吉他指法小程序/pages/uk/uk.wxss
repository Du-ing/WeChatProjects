.body{
  margin: 0rpx 10rpx 25rpx 25rpx;
}
.flex_row{
  display: flex;
  flex-direction: row;
  margin-bottom: 50rpx;
}
.active{
  display: inline-block;
  padding: 10rpx 10rpx;
  text-align: center;
  color: azure;
  font-weight: bold;
  width: 100rpx;
  background-color: springgreen;
  border: darkgreen 1rpx solid;
  border-radius: 15rpx;
}
.inactive{
  display: inline-block;
  padding: 10rpx 10rpx;
  text-align: center;
  color: azure;
  font-weight: bold;
  width: 100rpx;
  background-color: #228B22;
  border: darkgreen 1rpx solid;
  border-radius: 15rpx;
}
.foot{
  text-align: center;
  color: #808000;
  font-size: 35rpx;
  background-color: #F0E68C;
  margin-top: 380rpx;
}